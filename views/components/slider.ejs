<br>
<img id="vehicle" width="100%" src="/images/slider/1.jpg">
<script>
    $(document).ready(function () {
        arrImageSlider = [
            '/images/slider/1.jpg',
            '/images/slider/2.jpg',
            '/images/slider/3.jpg',
            '/images/slider/4.png'
        ]
        var image = $('#vehicle')
        var i = 0;
        setInterval(() => {
            j = i % 4
            image.fadeOut('slow', function () {
                image.attr('src', arrImageSlider[j]);
                image.fadeIn('fast');
                i++
            });
        }, 2000);
    });
</script>