<div class="col s12 l6 m6">
    <a href="../xe/<%- car.url %>">
        <div class="card z-depth-2 center-align">
            <div class="card-image grey lighten-2">
                <img src="<%- car.thumbnail %>">
            </div>
            <div class="card-content">
                <span class="red-text price">
                    <%- car.price %>
                </span>
            </div>
            <div class="card-action">
                <span class="card-title">
                    <h5>
                            <%- car.name %>
                    </h5>
                </span>
            </div>
        </div>
    </a>
</div>